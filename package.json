{
  "name": "adavu-client",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "ng": "ng",
    "prepare": "husky",
    "lint": "yarn lint:js && yarn lint:prettier && yarn lint:style",
    "lint:js": "eslint --ext \".ts\" --ignore-path .gitignore .",
    "lint:jsfix": "eslint --ext \".ts\" --ignore-path .gitignore . --fix",
    "lintfix": "yarn lint:jsfix && yarn lint:prettierfix && yarn  lint:stylefix",
    "lint:prettier": "prettier --check .",
    "lint:style": "npx stylelint \"src/asset/scss/custom/**/*.scss\" \"src/app/**/*.scss\"",
    "lint:stylefix": "npx stylelint \"src/asset/scss/custom/**/*.scss\" \"src/app/**/*.scss\" --fix",
    "lint:prettierfix": "prettier --write --list-different . ",
    "build": "ng build",
    "build:dev": "ng build --configuration devserver",
    "build:stg": "ng build --configuration staging",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "start": "ng serve --port 80",
    "start:dev": "ng serve  --port 4270",
    "start:ssr:docker": "ng build && node dist/client/server/server.mjs",
    "start:docker": "docker compose up --build client",
    "serve:ssr": "node dist/client/server/server.mjs",
    "test": "ng test"
  },
